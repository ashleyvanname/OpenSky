{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataLoaderv2"
		},
		"Destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Destination'"
		},
		"Source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Source'"
		},
		"Trigger_9yc_properties_CopyData_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@{addminutes(trigger().scheduledTime, -1)}"
		},
		"Trigger_9yc_properties_CopyData_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().scheduledTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Destination_connectionString": {
						"value": "[parameters('Destination_connectionString')]"
					},
					"Source_connectionString": {
						"value": "[parameters('Source_connectionString')]"
					},
					"Trigger_9yc_properties_CopyData_parameters_windowStart": {
						"value": "[parameters('Trigger_9yc_properties_CopyData_parameters_windowStart')]"
					},
					"Trigger_9yc_properties_CopyData_parameters_windowEnd": {
						"value": "[parameters('Trigger_9yc_properties_CopyData_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}